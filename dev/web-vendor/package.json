        "react-js-pagination": "^3.0.2",
        "react-moment": "^0.8.4",
        "react-responsive-carousel": "^3.1.43",
        "react-router-dom": "^4.3.1",
        "react-scripts-ts": "^4.0.8",
        "sass-loader": "^7.1.0",
